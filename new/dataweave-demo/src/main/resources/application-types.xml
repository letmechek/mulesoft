<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="employeeMetadataJSON" format="json">
      <types:shape format="weave" example="examples/employee.json"><![CDATA[%dw 2.0

type employeeMetadataJSON = {|
  empID: Number {"typeId": "int"}, 
  empName: String, 
  empEmail: String, 
  empStatus: String
|} {"example": "{\\\"empID\\\":78788,\\\"empName\\\":\\\"Chinna\\\",\\\"empEmail\\\":\\\"chinna.mulesoft@gmail.com\\\",\\\"empStatus\\\":\\\"A\\\"}"}

]]></types:shape>
    </types:type>
    <types:type name="bookMetaDataXML" format="xml">
      <types:example format="xml" element="book" location="examples/book.xml"/>
    </types:type>
    <types:type name="bookmetadataJSON" format="json">
      <types:shape format="weave" example="examples/book.json"><![CDATA[%dw 2.0

type bookmetadataJSON = {|
  item: {|
    "type": String, 
    price: Number {"typeId": "int"}, 
    properties: {|
      title: String, 
      author: Array<String>, 
      year: Number {"typeId": "int"}, 
      lungage: String
    |}
  |}
|} {"example": "{\\\"item\\\":{\\\"type\\\":\\\"book\\\",\\\"price\\\":30,\\\"properties\\\":{\\\"title\\\":\\\"Everyday Italian\\\",\\\"author\\\":[\\\"Giada De Laurentiis\\\"],\\\"year\\\":2005,\\\"lungage\\\":\\\"en\\\"}}}"}

]]></types:shape>
    </types:type>
    <types:type name="employeemetadataCSV" format="csv">
      <types:shape format="weave" example="examples/employee.csv"><![CDATA[%dw 2.0

type employeemetadataCSV = Array<{|  empID: String,   empName: String,   empEmail: String,   empStatus: String|}> {"isQuotedString": "false",
"header": "true",
"headerLineNumber": "0"}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
      </types:properties>
    </types:type>
  </types:catalog>
  <types:enrichment select="#0d590e34-d236-4f10-987a-eb0fa70d4454">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="employeeMetadataJSON"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="employeeMetadataJSON"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
  <types:enrichment select="#5bf8a4db-8b70-46df-898c-ccb3f1713313">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="bookMetaDataXML"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="bookmetadataJSON"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
  <types:enrichment select="#bf4fc031-5b17-472f-8329-e08e1ccd761c">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="bookmetadataJSON"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="bookMetaDataXML"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
  <types:enrichment select="#e38e85f6-1d37-4dc6-bc1f-f78cf4222495">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="employeeMetadataJSON"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="employeemetadataCSV"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
</types:mule>